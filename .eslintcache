;\r\n\r\n  useEffect(()=>{\r\n    db.collection('wine').onSnapshot(snapshot=>{\r\n        setStocks(snapshot.docs.map(doc=>({\r\n            id:doc.id,\r\n            stock:doc.data()})));\r\n    })\r\n},[])\r\n  return (\r\n    <div className=\"stock\">\r\n      {stocks.map(({id,stock}) => (\r\n        <StockItem\r\n          key={id}\r\n          id={id}\r\n          images={stock.images}\r\n          title={stock.title}\r\n          quantity={stock.quantity}\r\n          stock={stock.quantity}\r\n          description={stock.description}\r\n          price={stock.price}\r\n          rating={stock.rating}\r\n          type={stock.type}\r\n        />\r\n      ))}\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Stock;\r\n","D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Pages\\Home.jsx",["135","136","137"],"import React, { useContext } from \"react\";\r\nimport { db } from \"../Firebase/Firebase\";\r\nimport { UserContext } from \"../Services/Context/Auth\";\r\nimport { DbContext } from \"../Services/Context/Db\";\r\nconst Home = () => {\r\n  const user = useContext(UserContext);\r\n  const { addWine } = useContext(DbContext);\r\n  //const { email } = user;\r\n\r\n  const handleSubmit = () => {\r\n    let input = { title: \"test\", test: \"test\" };\r\n    db.collection(\"wine\").doc(\"wine1\").get();\r\n  };\r\n  return (\r\n    <div className=\"home\">\r\n      <h1>{\"email\"}</h1>\r\n      <button onClick={handleSubmit}>Check</button>\r\n    </div>\r\n  );\r\n};\r\nexport default Home;\r\n","D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Navbar\\navbar.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Login\\login.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Services\\Reducers\\AppReducer.js",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Services\\Reducers\\InventoryReducer.js",["138"],"import Input from \"../../Pages/Input\";\r\n\r\nconst InventoryReducer = (state, action) => {\r\n  console.log(\"action\", action);\r\n  switch (action.type) {\r\n    case \"CHANGE_INPUT_STATE\":\r\n      return {\r\n        ...state,\r\n        input: {\r\n          ...state.input,\r\n          ...action.payload,\r\n        },\r\n      };\r\n    case \"CHANGE_STOCK_STATE\": {\r\n      let item;\r\n      for (let i = 0; i < state.stock.length; i++) {\r\n        if (state.stock[i].id === action.payload.id) {\r\n          item = {\r\n            ...state.stock[i],\r\n            ...action.payload.input,\r\n          };\r\n          state.stock.splice(i, 1, item);\r\n        }\r\n      }\r\n      return { ...state };\r\n    }\r\n    default:\r\n      return state;\r\n  }\r\n};\r\n\r\nexport default InventoryReducer;\r\n","D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\description.jsx",[],["139","140"],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\addImage.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\price.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\images.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\quantity.jsx",["141","142"],"import React, { useContext } from \"react\";\r\nimport { InventoryContext } from \"../../Services/Context/Inventory\";\r\n\r\nconst Quantity = ({ quantity, setQuantity }) => {\r\n  return (\r\n    <>\r\n      <label>Quantity</label>\r\n      <input\r\n        type=\"number\"\r\n        value={quantity}\r\n        onChange={(e) => setQuantity({ quantity: parseInt(e.target.value) })}\r\n      />\r\n    </>\r\n  );\r\n};\r\nexport default Quantity;\r\n","D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\rating.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\title.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\Input\\type.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\functions\\remove.jsx",[],"D:\\React-22-01\\D-Vine-Admin-Panel\\src\\Components\\StockItem\\stockitem.jsx",["143","144","145"],"import React, { useContext, useState } fr